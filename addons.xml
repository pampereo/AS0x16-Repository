<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.AS0x16.xbmc-addons" name="AS0x16 XBMC Add-on Repository" version="1.0.0" provider-name="AS0x16">
    <extension point="xbmc.addon.repository" name="AS0x16 XBMC Add-on Repository">
        <info compressed="false">https://raw.githubusercontent.com/pampereo/AS0x16-Repository/master/addons.xml</info>
